{
    "name": "ppinot-visual",
    "version": "0.5.0",
    "description": "A visual modeler of Process Performance Indicators based on PPINOT and built with bpmn-js",
    "scripts": {
        "all": "run-s lint build",
        "build": "webpack --mode production",
        "build:hybrid": "webpack --mode production",
        "dev": "webpack-dev-server --open",
        "dev:hybrid": "webpack-dev-server --open --env target=hybrid",
        "lint": "eslint .",
        "start": "run-s dev",
        "start:hybrid": "run-s dev:hybrid"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/isa-group/ppinot-visual"
    },
    "keywords": [
        "ppinot",
        "visual ppinot",
        "bpmn",
        "ppis"
    ],
    "author": {
        "name": "ISA Group",
        "url": "https://github.com/isa-group"
    },
    "contributors": [
        {
            "name": "ISA Group contributors",
            "url": "https://github.com/isa-group"
        }
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.26.7",
        "@babel/preset-env": "^7.26.7",
        "babel-loader": "^9.2.1",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^6.5.1",
        "eslint-plugin-bpmn-io": "^0.10.0",
        "ids": "latest",
        "json-loader": "^0.5.7",
        "npm-run-all": "^4.1.5",
        "raw-loader": "^4.0.2",
        "style-loader": "^4.0.0",
        "webpack": "^5.88.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.2"
    },
    "dependencies": {
        "bpmn-js": "^18.6.2",
        "bpmn-moddle": "latest",
        "diagram-js": "latest",
        "inherits": "latest",
        "jquery": "^3.4.1",
        "min-dash": "^4.2.2",
        "ppinot-visual": "file:",
        "tiny-svg": "latest"
    }
    
}
